{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "regions": ["sfo1"],
  "env": {
    "NEXT_PUBLIC_SITE_URL": "${NEXT_PUBLIC_SITE_URL}",
    "NEXT_PUBLIC_COMPANY_NAME": "${NEXT_PUBLIC_COMPANY_NAME}",
    "NEXT_PUBLIC_COMPANY_PHONE": "${NEXT_PUBLIC_COMPANY_PHONE}",
    "NEXT_PUBLIC_COMPANY_EMAIL": "${NEXT_PUBLIC_COMPANY_EMAIL}"
  }
}